<?xml version="1.0"?>
<extension key="de.systopia.betterplace" type="module">
  <file>betterplace</file>
  <name>betterplace.org Direkt API</name>
  <description>Integration for the betterplace.org Direkt donation page</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.betterplace</url>
    <url desc="Documentation">https://docs.civicrm.org/betterplace/en/latest</url>
    <url desc="Support">https://github.com/systopia/de.systopia.betterplace/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <requires>
    <ext>de.systopia.xcm</ext>
  </requires>
  <civix>
    <namespace>CRM/Betterplace</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Betterplace_Upgrader</upgrader>
</extension>
